<Application x:Class="HomeCenter.NET.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HomeCenter.NET"
             xmlns:converters="clr-namespace:HomeCenter.NET.Converters"
             Startup="Application_Startup"
             Exit="Application_Exit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:Bootstrapper x:Key="Bootstrapper" />
                    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
                    <converters:BooleanToGreenRedColorConverter x:Key="BooleanToGreenRedColorConverter" />
                    <converters:BooleanToGreenBisqueColorConverter x:Key="BooleanToGreenBisqueColorConverter" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
